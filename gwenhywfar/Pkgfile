# Depends on: 

name=gwenhywfar
version=4.2.0
release=1
source=("http://www2.aquamaniac.de/sites/download/download.php?package=01&release=62&file=01&dummy=$name-$version.tar.gz")

build(){
	cd $name-$version
	./configure --prefix=/usr 
	make 
	make DESTDIR=$PKG install
	rm -rf $PKG/usr/share/locale

}

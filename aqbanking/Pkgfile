# Description: Generic OnlineBanking interface
# URL: http://www2.aquamaniac.de/sites/aqbanking/index.php
# Maintainer: anton baranov, abaranov dot anton at gmail dot com	
# Depends on: gwenhywfar

name=aqbanking
version=5.0.22
release=1
source=("http://www2.aquamaniac.de/sites/download/download.php?package=03&release=92&file=01&dummy=$name-$version.tar.gz")

build(){
	cd $name-$version
	./configure --prefix=/usr 

	make 
	make DESTDIR=$PKG install

	rm -rf $PKG/usr/share/{doc,locale}

}
